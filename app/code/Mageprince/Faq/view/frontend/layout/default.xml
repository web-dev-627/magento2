<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
            <block class="Mageprince\Faq\Block\HeaderLink" name="faq-header-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">FAQ</argument>
                    <argument name="path" xsi:type="helper" helper="Mageprince\Faq\Helper\Data::getFaqUrl" />
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="footer_links">
            <block class="Mageprince\Faq\Block\FooterLink" name="faq-footer-link" after="privacy-policy-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">FAQ</argument>
                    <argument name="path" xsi:type="helper" helper="Mageprince\Faq\Helper\Data::getFaqUrl" />
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
