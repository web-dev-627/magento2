<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="img-src">
            <values>
                <value id="quickchart" type="host">quickchart.io</value>
                <value id="dataimage" type="host">data:</value>
                <value id="imgyoutube" type="host">img.youtube.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="maxcdn.bootstrapcdn" type="host">maxcdn.bootstrapcdn.com</value>
                <value id="fonts.gstatic" type="host">fonts.gstatic.com</value>
                <value id="fonts.googleapis" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="maxcdn.bootstrapcdn" type="host">maxcdn.bootstrapcdn.com</value>
                <value id="fonts.gstatic" type="host">fonts.gstatic.com</value>
                <value id="fonts.googleapis" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
